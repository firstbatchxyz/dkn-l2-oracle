{
  "name": "LLM generation",
  "description": "Directly generate text with input",
  "config": {
    "max_steps": 10,
    "max_time": 50,
    "tools": [""]
  },
  "external_memory": {},
  "tasks": [
    {
      "id": "A",
      "name": "Generate",
      "description": "Executes a simple generation request",
      "operator": "generation",
      "messages": [
        {
          "role": "user",
          "content": "{{text}}"
        }
      ],
      "inputs": [
        {
          "name": "text",
          "value": {
            "type": "input",
            "key": ""
          },
          "required": true
        }
      ],

      "outputs": [
        {
          "type": "write",
          "key": "result",
          "value": "__result"
        }
      ]
    },
    {
      "id": "__end",
      "name": "end",
      "description": "End of the task",
      "messages": [{ "role": "user", "content": "End of the task" }],
      "inputs": [],
      "operator": "end",
      "outputs": []
    }
  ],
  "steps": [
    {
      "source": "A",
      "target": "__end"
    }
  ],
  "return_value": {
    "input": {
      "type": "read",
      "key": "result"
    }
  }
}
